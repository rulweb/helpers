{
    "name": "rulweb/laravel-helpers",
    "description": "Хелперы для Laravel Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "RuleZzz",
            "email": "rul22rus@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "laravel/framework": "^8.0",
        "ext-intl": "*",
        "ext-json": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "files":  [
            "src/datetime.php",
            "src/json.php",
            "src/price.php",
            "src/user.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
